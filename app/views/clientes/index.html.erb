<p id="notice"><%= notice %></p>

<h1>Clientes</h1>
<%= link_to 'Crear Cliente', new_cliente_path, class: "btn btn-info" %>
<%= link_to 'Regresar a CatÃ¡logos', catalogos_path, class: "btn btn-success" %>
<hr>

<table>
  <thead>
    <tr>
      <th>Nombre comercial</th>
      <th>Rfc</th>
      <th>Colonia</th>
      <th>Estado</th>
      <th>Telefono oficina</th>
      <th>Telefono celular</th>
      <th>Telefono directo</th>
      <th>Correo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nombre_comercial %></td>
        <td><%= cliente.rfc %></td>
        <td><%= cliente.colonia %></td>
        <td><%= cliente.estado %></td>
        <td><%= cliente.telefono_oficina %></td>
        <td><%= cliente.telefono_celular %></td>
        <td><%= cliente.telefono_directro %></td>
        <td><%= cliente.correo %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to "Editar", edit_cliente_path(cliente), class: "glyphicon glyphicon-pencil"  %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  </tbody>

</table>

<br>
