<p id="notice"><%= notice %></p>

<h1>Tipo Servicios</h1>

<table>
  <thead>
    <tr>
      <th>Cve tipo servicio</th>
      <th>Detalle servicio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_servicios.each do |tipo_servicio| %>
      <tr>
        <td><%= tipo_servicio.cve_tipo_servicio %></td>
        <td><%= tipo_servicio.detalle_servicio %></td>
        <td><%= link_to 'Show', tipo_servicio %></td>
        <td><%= link_to 'Edit', edit_tipo_servicio_path(tipo_servicio) %></td>
        <td><%= link_to 'Destroy', tipo_servicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Servicio', new_tipo_servicio_path %>
