<%= simple_form_for(@solicitud_tuberium) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id_solicitud %>
    <%= f.input :piso %>
    <%= f.input :altura_piso %>
    <%= f.input :codigo_evaluacion %>
    <%= f.input :diametro %>
    <%= f.input :no_uniones %>
    <%= f.input :cve_cedula %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
